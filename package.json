{
  "name": "react-ultra-date-picker",
  "version": "1.1.6",
  "description": "An iOS like, comprehensive date picker component for React",
  "main": "dist/react-ultra-date-picker.js",
  "scripts": {
    "build-dist": "webpack --config webpack.config.js --progress --colors",
    "build": "npm run lint && npm run build-dist&&export NODE_ENV=production&&npm run build-dist",
    "prepublish": "npm run build",
    "examples": "webpack-dev-server --content-base examples/ --config examples/webpack.config.js --progress --colors --hot --inline",
    "build-demo": "rm -rf demo&&export NODE_ENV=production&&webpack --config examples/webpack.config.prod.js --progress --colors",
    "lint": "./node_modules/.bin/eslint src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swenyang/react-date-picker.git"
  },
  "keywords": [
    "react",
    "date",
    "picker",
    "day",
    "ios",
    "mobile"
  ],
  "author": "swenyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swenyang/react-date-picker/issues"
  },
  "homepage": "https://github.com/swenyang/react-date-picker#readme",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-ultra-select": "~1.0.14"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.19.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
